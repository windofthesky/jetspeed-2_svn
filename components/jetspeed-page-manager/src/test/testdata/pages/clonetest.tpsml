<page-template id="clonetest"
    xmlns="http://portals.apache.org/jetspeed"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://portals.apache.org/jetspeed http://portals.apache.org/jetspeed-2/2.1/schemas/psml.xsd">

  <title>Clone Test Page Template</title>
  <metadata name="description">Clone Test Page Template Description</metadata>
  <defaults skin="test-template-skin" layout-decorator="test-template-layout" portlet-decorator="test-template-portlet"/>

  <fragment id="cpt-f001" type="layout" name="TwoColumns">
