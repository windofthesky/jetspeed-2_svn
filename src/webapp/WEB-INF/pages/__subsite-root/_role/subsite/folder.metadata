<?xml version="1.0" encoding="UTF-8"?>
<folder
    xmlns="http://portals.apache.org/jetspeed"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://portals.apache.org/jetspeed http://portals.apache.org/jetspeed-2/2.1/schemas/folder-metadata.xsd">

  <title>Subsite Root Folder</title>  
  <default-page>subsite-default-page.psml</default-page>
  <metadata name="title" xml:lang="ja">サブサイトルートフォルダ</metadata>
  <metadata name="title" xml:lang="es">Carpeta raiz de subportal</metadata>
  <metadata name="title" xml:lang="zh">子站点根文件夹</metadata>
  <metadata name="title" xml:lang="zh-TW">子站點根文件夾</metadata>
  

  <security-constraints>
    <owner>subsite</owner>
    <security-constraint>
      <roles>subsite, subsite2</roles>
      <permissions>view</permissions>
    </security-constraint>
  </security-constraints>

</folder>
