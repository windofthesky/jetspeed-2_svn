      <version>${portals.bridges.velocity.version}</version>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
        <id>portals-bridges:portals-bridges-jsf</id>
        <version>${portals.bridges.jsf.version}</version>
        <properties>
            <war.bundle>true</war.bundle>
        </properties>
    </dependency>
    
    <!-- Servlets, JSTL -->      
    <dependency>
        <id>jstl</id>
        <version>1.0.6</version>
        <type>jar</type>
        <properties>
            <war.bundle>true</war.bundle>
